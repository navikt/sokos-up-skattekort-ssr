{
	"name": "sokos-up-skattekort-ssr",
	"type": "module",
	"version": "1.0.0",
	"scripts": {
		"astro": "astro",
		"dev": "astro dev",
		"dev:mock": "concurrently \"pnpm dev\" \"tsx mock/server.ts\"",
		"build": "astro check && astro build",
		"biome:check": "biome check --files-ignore-unknown=true",
		"biome:fix": "biome check --write --no-errors-on-unmatched",
		"prettier:format": "prettier --write '**/*.astro'",
		"stylelint:check": "stylelint '**/*.css'",
		"stylelint:fix": "stylelint '**/*.css' --fix",
		"prepare": "husky"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.6",
		"@astrojs/node": "^9.5.3",
		"@astrojs/react": "^4.4.2",
		"@navikt/aksel-icons": "8.5.2",
		"@navikt/ds-css": "8.5.2",
		"@navikt/ds-react": "8.5.2",
		"@navikt/oasis": "^4.1.4",
		"astro": "^5.17.2",
		"pino-http": "^11.0.0",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.0",
		"@hono/node-server": "1.19.9",
		"@navikt/aksel-stylelint": "^8.5.2",
		"@types/node": "25.2.3",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"baseline-browser-mapping": "^2.9.19",
		"concurrently": "^9.2.1",
		"hono": "4.11.9",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"postcss": "^8.5.6",
		"postcss-prefix-selector": "^2.1.1",
		"prettier": "3.8.1",
		"prettier-plugin-astro": "^0.14.1",
		"stylelint": "^16.26.1",
		"stylelint-config-standard": "^39.0.1",
		"tsx": "4.21.0",
		"typescript": "5.9.3"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": "biome check --write --no-errors-on-unmatched",
		"*.astro": "prettier --write",
		"*.css": "stylelint --fix"
	},
	"engines": {
		"pnpm": ">=10.16.0",
		"node": ">=24.13.0"
	}
}
